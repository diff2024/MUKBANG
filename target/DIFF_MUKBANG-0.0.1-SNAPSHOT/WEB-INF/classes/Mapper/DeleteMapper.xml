<!DOCTYPE mapper
	PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.diff.map.Mapper.DeleteMapper">

	<delete id="ChannelDelete" parameterType="HashMap">
		DELETE FROM user_channel_info
		 WHERE _user_id = #{user_id} 
		   AND _user_channel_id = #{channel_id}
	</delete>
	
</mapper>